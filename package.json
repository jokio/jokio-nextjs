{
	"name": "jokio-nextjs",
	"version": "0.2.0",
	"author": "Jok Entertainers",
	"description": "nextjs middleware for jokio",
	"main": "dist/index.js",
	"types": "dist/index.d.js",
	"scripts": {
		"start": "ts-node src",
		"test": "ts-node src/_test",
		"dev": "nodemon",
		"build": "tsc"
	},
	"dependencies": {
		"express": "^4.16.2",
		"jokio": "^0.2.1",
		"lodash": "^4.17.4",
		"next": "4.2.3",
		"react": "16.2.0",
		"react-dom": "16.2.0"
	},
	"devDependencies": {
		"@types/express": "^4.11.0",
		"@types/jest": "22.1.0",
		"@types/lodash": "^4.14.97",
		"jest": "22.1.4",
		"nodemon": "1.14.11",
		"ts-jest": "22.0.1",
		"ts-node": "4.0.1",
		"typescript": "2.6.2"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"jok"
	],
	"license": "MIT",
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
